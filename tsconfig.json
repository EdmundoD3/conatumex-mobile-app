{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": false,
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@database/*": ["./database/*"],
      "@constants/*": ["./helpers/constants/*"],
      "@error/*": ["./helpers/errors/*"],
      "@helpers/*": ["./helpers/*"],
      "@screens/*": ["./src/components/screen/*"],
      "@ui/*":["./src/components/ui/*"],
      "@services/*":["./services/*"],
      "@models/*":["./helpers/models/*"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
